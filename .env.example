MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/chronos-system
MONGODB_DB_NAME=chronos_system

# PostgreSQL
DATABASE_URL=postgresql://user:password@localhost:5432/chronos_system

# ============== WEBHOOKS ==============
